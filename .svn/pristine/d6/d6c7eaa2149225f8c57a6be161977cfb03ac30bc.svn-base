@mixin prefix($name,$vlaue) {
    -webkit-#{$name}: $vlaue;
    #{$name}: $vlaue;
}
@mixin transition-left($vlaue){
    @include prefix(transition,$vlaue)
}



@mixin shadow($vlaue){
    @include prefix(box-shadow,$vlaue)
}